						<!-- <span><%#= render partial: 'recording/desktop/recording' %></span> -->
					</div>
					<div class="col-sm-12 col-md-12 col-lg-2 text-center">
					
					</div>
				</div>
			</span>	
			
			
			<span id="visitor-video-detail-span" style="display:none;">
				<form class="form-horizontal" id="save-video-details-form">
				<input type="hidden" name= "video_detail_session_id">
				<div class="row">
					<div class="col-sm-12 col-md-12 col-lg-2 text-right" style="padding-top:20%;">
						
					</div>
					<div class="col-sm-12 col-md-12 col-lg-8 text-center">
						<%= render partial: 'recording/desktop/video_details' %>
					</div>
					<div class="col-sm-12 col-md-12 col-lg-2 text-right" style="padding-top:20%;">
						
					</div>
				
				</div>
				<div class="row">
					<div class="col-sm-12 col-md-12 col-lg-2 text-center">
					<span id="re-recording-message" style="display:inline;">
					<p class=""><!-- Do you want to re-record the message? --><a href="/create/video" id="re-record" class="btn btn-danger" style="cursor: pointer; cursor: hand; color:#FFF; font-weight:bold;" onclick="return confirm('Clicking Back button will start a new video recording. Are you sure?')">Back</a></p>
					</span>
					</div>
					<div class="col-sm-12 col-md-12 col-lg-8">
						<!-- <span><%#= render partial: 'recording/desktop/recording' %></span> -->
					</div>
					<div class="col-sm-12 col-md-12 col-lg-2 text-center">
					<span id="re-record-span" style="display:inline;">
					<p class=""><a href="#" id="video-detail-next-btn" class="btn btn-danger" style="cursor: pointer; cursor: hand;  color:#FFF; font-weight:bold;"><strong>Next</strong></a></p>
						<!-- <button type="submit" class="btn btn-danger" style="cursor: pointer; cursor: hand;  color:#FFF; font-weight:bold;"><strong>Submit</strong></button> -->
					<!-- <p>If you are happy with your recording, click Next.</p> -->
					</span>
					</div>
				</div>
			</span>
					
					
			<span id="visitor-scheduler-detail-span" style="display:none;">
			<input type="hidden" name= "video_schedule_session_id">
			<input type="hidden" name= "visitor_id">
			<div class="row">
				<div class="col-sm-12 col-md-12 col-lg-2 text-right" style="padding-top:20%;">
					
				</div>
				<div class="col-sm-12 col-md-12 col-lg-8 text-center">
					<%= render partial: 'recording/desktop/video_schedule' %>
