					<div class="col-sm-12 col-md-12 col-lg-2 text-right" style="padding-top:20%;">
						<span id="re-recording-message" style="display:none;">
							<p class=""><!-- Do you want to re-record the message? --><a href="/create/video" id="re-record" class="btn btn-danger" style="cursor: pointer; cursor: hand; color:#FFF; font-weight:bold;">Re-Record</a></p>
						</span>
					</div>
					<div class="col-sm-12 col-md-12 col-lg-8 text-center">
						<%= render partial: 'recording/desktop/recording' %>
					</div>
					<div class="col-sm-12 col-md-12 col-lg-2 text-right" style="padding-top:20%;">
						<span id="re-record-span" style="display:none;">
							<p class=""><a href="#" id="recording-next-btn" class="btn btn-danger" style="cursor: pointer; cursor: hand;  color:#FFF; font-weight:bold;"><strong>Next</strong></a></p>
						</span>
					</div>
				
				</div>
				<div class="row">
					<div class="col-sm-12 col-md-12 col-lg-2 text-center">
					
					</div>
