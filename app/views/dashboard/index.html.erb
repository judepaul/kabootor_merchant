<div class="container-fluid fullHeight">
  <div class="row jumbotron">
	  <div class="row"><br/></div>
	  <% if current_user.blank? %>
	  <% if @visitor.blank? %>
	  	<div class="row"><br/></div>
	  	<%= render partial: "dashboard/shared/pre_video_creation" %>
	  <% else %>
		<%= render partial: "dashboard/shared/post_video_creation", :locals => {:@visitor=>@visitor} %>
	  <% end %>
	  <% else %>
	  	<%= render partial: "dashboard/shared/post_video_creation", :locals => {:@visitor=>current_user.visitor} %>
	  <% end %>
  </div>
</div>



